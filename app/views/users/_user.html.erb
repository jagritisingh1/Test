 <tr id="User_row_<%=user.id %>">
      <td><%= user.name%></td>
      <td><%= user.email%></td>
      <td><%= user.city%></td>
      <td><%= user.contact_number%></td>
      <td><%= user.state%></td>
      <td><%= user.country%></td>
      <td>
        <%= link_to "View", user, class: "btn btn-info" %>
        <%= link_to "Edit", edit_user_path(user), class: "btn btn-success" %>
        <%= link_to "Delete", user, method: :delete, data: { confirm: 'Are you sure you want to delete this user'}, class: "btn btn-danger" %>
      </td>
    </tr>